<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- namespace,指定要实现的接口 全限定名 -->
<mapper namespace="com.zyzh.dao.DataBaseDao">
  <select id="selectDataBaseById" resultType="DataBase">
      select id,url,username,passward,driver,sqlxx from x_database where id=#{id}
  </select>
</mapper>
